{
  "functions": {
    "api/*.js": {
      "maxDuration": 10,
      "runtime": "nodejs18.x"
    },
    "api/*.py": {
      "maxDuration": 30,
      "memory": 1024,
      "runtime": "python@3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/query",
      "destination": "/api/query.js"
    },
    {
      "source": "/api/ask",
      "destination": "/api/ask-direct.py"
    }
  ]
}
